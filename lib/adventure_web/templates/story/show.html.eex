<div class="story-img">
  <img class="story-img-art" src='<%= @image %>' alt='<%= @story.search_request %>'>
</div>

<h1 style="color:black"><%= @story.search_request %></h1>

<div class="adventure-story">
  <%= @text %>
</div>

<div class="adventure-story">
  <p class="type-text"></p>
</div>

<script src="<%= static_path(@conn, "/js/app.js") %>"></script>
<script>require('js/app.js').App.typeit(`<%= @text %>`);</script>
